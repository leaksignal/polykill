{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"rootDir": "./generated-src",
		"outDir": "dist",
		"sourceMap": true,
		"declaration": true,
		"declarationMap": true,
		"incremental": true
	},
	"include": ["generated-src"],
	"exclude": ["node_modules", "leakscanner"]
}
